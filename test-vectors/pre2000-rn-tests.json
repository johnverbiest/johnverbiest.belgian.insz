{
  "name": "pre-2000 RN validator tests",
  "description": "Working and failing Belgian Rijksregisternummer (pre-2000 rule only). No BIS or special cases included; all months are 01–12 and we avoid BIS offsets (+20/+40).",
  "testCases": [
    {
      "testName": "Valid male RN 1985",
      "testDescription": "Checks a normal male RN born 13 June 1985 with valid checksum (pre-2000 rule).",
      "input": "85061300178",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "1985-06-13",
        "birthYear": 1985,
        "sex": "M"
      },
      "because": "Valid RN: 13/06/1985, seq 001 (odd), checksum uses pre-2000 rule."
    },
    {
      "testName": "Valid female RN 1979",
      "testDescription": "Checks a normal female RN born 31 December 1979 with valid checksum (even sequence).",
      "input": "79123100235",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "1979-12-31",
        "birthYear": 1979,
        "sex": "F"
      },
      "because": "Valid RN: 31/12/1979, seq 002 (even), correct checksum (pre-2000)."
    },
    {
      "testName": "Valid male RN 1999 end of range",
      "testDescription": "Boundary test for year 1999 to ensure checksum still uses pre-2000 rule.",
      "input": "99010199789",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "1999-01-01",
        "birthYear": 1999,
        "sex": "M"
      },
      "because": "Valid RN: 01/01/1999, seq 997 (odd), correct checksum (pre-2000)."
    },
    {
      "testName": "Valid female RN 1901",
      "testDescription": "Earliest century example to confirm correct century inference (1900s).",
      "input": "01022899840",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "1901-02-28",
        "birthYear": 1901,
        "sex": "F"
      },
      "because": "Valid RN: 28/02/1901, seq 998 (even), correct checksum (pre-2000)."
    },
    {
      "testName": "Valid leap year RN 1980",
      "testDescription": "Leap year date (1980) to confirm date validation correctly handles leap day.",
      "input": "80022800276",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "1980-02-28",
        "birthYear": 1980,
        "sex": "F"
      },
      "because": "Valid RN: 28/02/1980, seq 002 (even), correct checksum (pre-2000)."
    },
    {
      "testName": "Valid male RN 1950",
      "testDescription": "Mid-range RN to confirm consistent checksum and parity logic.",
      "input": "50113012307",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "1950-11-30",
        "birthYear": 1950,
        "sex": "M"
      },
      "because": "Valid RN: 30/11/1950, seq 123 (odd), correct checksum (pre-2000)."
    },

    {
      "testName": "Invalid short length",
      "testDescription": "Detects inputs shorter than 11 digits.",
      "input": "8506130017",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid length: only 10 digits (RN must be exactly 11 digits)."
    },
    {
      "testName": "Invalid long length",
      "testDescription": "Detects inputs longer than 11 digits.",
      "input": "850613001717",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid length: 12 digits (RN must be exactly 11 digits)."
    },
    {
      "testName": "Invalid character non-digit",
      "testDescription": "Ensures only digits are accepted.",
      "input": "8506130017A",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid character: contains a non-digit (letters are not allowed)."
    },
    {
      "testName": "Invalid date Feb 31",
      "testDescription": "Rejects impossible calendar dates.",
      "input": "99023100195",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid date: 31/02/1999 does not exist (checksum would otherwise fit pre-2000)."
    },
    {
      "testName": "Invalid month 13",
      "testDescription": "Rejects month values outside 01–12 (non-BIS).",
      "input": "85131300130",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid month: 13 is outside 01–12 and not a BIS offset."
    },
    {
      "testName": "Invalid day 00",
      "testDescription": "Rejects zero day values.",
      "input": "85060000180",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid day: 00 is not a valid calendar day for RN."
    },
    {
      "testName": "Invalid date non-leap year",
      "testDescription": "Rejects February 29th on non-leap years.",
      "input": "99022900158",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid date: 29/02/1999 is not a leap day (1999 not leap year)."
    },
    {
      "testName": "Invalid day 32",
      "testDescription": "Rejects out-of-range day values (no admin-day use).",
      "input": "85113200128",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid day: 32 in November is not valid and not an admin-day encoding."
    },
    {
      "testName": "Invalid sequence 000",
      "testDescription": "Sequence 000 is unassigned.",
      "input": "85061300079",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid sequence: 000 is not assigned for RN."
    },
    {
      "testName": "Invalid sequence 999",
      "testDescription": "Sequence 999 is reserved and invalid.",
      "input": "85061399950",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid sequence: 999 is not assigned for RN."
    },
    {
      "testName": "Invalid checksum",
      "testDescription": "Correct date and sequence but wrong modulo-97 checksum.",
      "input": "85061300179",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid checksum: digits base are fine but control digits are wrong."
    },
    {
      "testName": "Invalid month 00",
      "testDescription": "Rejects month zero for RN (no BIS offset).",
      "input": "85000000138",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid month: 00 is not allowed for RN (and we are not using BIS fallbacks)."
    }
  ]
}
