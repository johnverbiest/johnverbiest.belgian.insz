{
  "name": "post-2000 RN validator tests",
  "description": "Working and failing Belgian Rijksregisternummer (post-2000 rule). No BIS or special cases included; all months are 01–12 and we avoid BIS offsets (+20/+40). Uses post-2000 checksum calculation with 2-prefix.",
  "testCases": [
    {
      "testName": "Valid male RN 2005",
      "testDescription": "Checks a normal male RN born 13 June 2005 with valid checksum (post-2000 rule).",
      "input": "05061300176",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "2005-06-13",
        "birthYear": 2005,
        "sex": "M"
      },
      "because": "Valid RN: 13/06/2005, seq 001 (odd), checksum uses post-2000 rule (2-prefix)."
    },
    {
      "testName": "Valid female RN 2010",
      "testDescription": "Checks a normal female RN born 31 December 2010 with valid checksum (even sequence).",
      "input": "10123100270",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "2010-12-31",
        "birthYear": 2010,
        "sex": "F"
      },
      "because": "Valid RN: 31/12/2010, seq 002 (even), correct checksum (post-2000)."
    },
    {
      "testName": "Valid male RN 2023",
      "testDescription": "Recent date test to ensure checksum uses post-2000 rule.",
      "input": "23010199774",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "2023-01-01",
        "birthYear": 2023,
        "sex": "M"
      },
      "because": "Valid RN: 01/01/2023, seq 997 (odd), correct checksum (post-2000)."
    },
    {
      "testName": "Valid female RN 2000",
      "testDescription": "Boundary test for year 2000 to ensure checksum uses post-2000 rule.",
      "input": "00022899848",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "2000-02-28",
        "birthYear": 2000,
        "sex": "F"
      },
      "because": "Valid RN: 28/02/2000, seq 998 (even), correct checksum (post-2000)."
    },
    {
      "testName": "Valid leap year RN 2020",
      "testDescription": "Leap year date (2020) to confirm date validation correctly handles leap day.",
      "input": "20022900276",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "2020-02-29",
        "birthYear": 2020,
        "sex": "F"
      },
      "because": "Valid RN: 29/02/2020, seq 002 (even), correct checksum (post-2000)."
    },
    {
      "testName": "Valid male RN 2015",
      "testDescription": "Mid-range RN to confirm consistent checksum and parity logic.",
      "input": "15113012377",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": "2015-11-30",
        "birthYear": 2015,
        "sex": "M"
      },
      "because": "Valid RN: 30/11/2015, seq 123 (odd), correct checksum (post-2000)."
    },
    {
      "testName": "Valid male RN 2004 - Only year is known",
      "testDescription": "Validates an RN where only the birth year is known (month and day are 00). This is a valid administrative encoding used when exact birth date is unknown.",
      "input": "04000029116",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": null,
        "birthYear": 2004,
        "sex": "M"
      },
      "because": "Valid RN: year 2004, month/day 00/00 (unknown date), seq 291 (odd = male), correct checksum (post-2000)."
    },
    {
      "testName": "Valid male RN 2000 - Unknown birth date",
      "testDescription": "Validates an RN where the birth date is completely unknown. Uses the administrative encoding 01/00/2000 as specified in the official documentation.",
      "input": "00010000197",
      "expected": {
        "isValid": true,
        "isBis": false,
        "birthDate": null,
        "birthYear": null,
        "sex": "M"
      },
      "because": "Valid RN: 01/00/2000 (unknown birth date encoding), seq 001 (odd = male), correct checksum (post-2000)."
    },
    
    {
      "testName": "Invalid short length",
      "testDescription": "Detects inputs shorter than 11 digits.",
      "input": "0506130016",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid length: only 10 digits (RN must be exactly 11 digits)."
    },
    {
      "testName": "Invalid long length",
      "testDescription": "Detects inputs longer than 11 digits.",
      "input": "050613001610",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid length: 12 digits (RN must be exactly 11 digits)."
    },
    {
      "testName": "Invalid character non-digit",
      "testDescription": "Ensures only digits are accepted.",
      "input": "0506130016A",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid character: contains a non-digit (letters are not allowed)."
    },
    {
      "testName": "Invalid date Feb 31",
      "testDescription": "Rejects impossible calendar dates.",
      "input": "20023100178",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid date: 31/02/2020 does not exist."
    },
    {
      "testName": "Invalid month 13",
      "testDescription": "Rejects month values outside 01–12 (non-BIS).",
      "input": "05131300113",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid month: 13 is outside 01–12 and not a BIS offset."
    },
    {
      "testName": "Invalid day 00",
      "testDescription": "Rejects zero day values.",
      "input": "05060000163",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid day: 00 is not a valid calendar day for RN."
    },
    {
      "testName": "Invalid date non-leap year",
      "testDescription": "Rejects February 29th on non-leap years.",
      "input": "19022900141",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid date: 29/02/2019 is not a leap day (2019 not leap year)."
    },
    {
      "testName": "Invalid day 32",
      "testDescription": "Rejects out-of-range day values (no admin-day use).",
      "input": "05113200111",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid day: 32 in November is not valid and not an admin-day encoding."
    },
    {
      "testName": "Invalid sequence 000",
      "testDescription": "Sequence 000 is unassigned.",
      "input": "05061300062",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid sequence: 000 is not assigned for RN."
    },
    {
      "testName": "Invalid sequence 999",
      "testDescription": "Sequence 999 is reserved and invalid.",
      "input": "05061399933",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid sequence: 999 is not assigned for RN."
    },
    {
      "testName": "Invalid checksum",
      "testDescription": "Correct date and sequence but wrong modulo-97 checksum.",
      "input": "05061300162",
      "expected": {
        "isValid": false,
        "isBis": null,
        "birthDate": null,
        "birthYear": null,
        "sex": null
      },
      "because": "Invalid checksum: digits base are fine but control digits are wrong."
    }
  ]
}
